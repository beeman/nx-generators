import { ExternalLinkIcon } from '@chakra-ui/icons';
import { Link } from '@chakra-ui/react';

export function UiLinkExternal({
  href,
  label,
}: {
  href: string;
  label: string;
}) {
  return (
    <Link color="primary.500" href={href} isExternal>
      {label} <ExternalLinkIcon mx="2px" />
    </Link>
  );
}
